    <body>

        <div id="navbar">
            <h4 id="title" class="nav">Admin Panel</h4>
            <a href="" id="displayStudentChoices" class="toggle">View Student Choices</a>
            <a href="" id="displayCompanyChoices" class="toggle">View Company Choices</a>
            <a href='sorter.php'>Go to Sorter</a>
            <a id="logOut" href='' class="nav">Log Out</a>
       </div>

        <div id="flexbox">
            <div id="mainContainer">
                <div id="companyChoices" class="box">
                    {{#if companyChoicesTable}}
                      {{{companyChoicesTable}}}
                    {{else}}
                      {{> companyChoicesBlank }}
                    {{/if}}
                </div>
                <div id="studentChoices" class="box">
                  {{#if studentChoicesTable}}
                    {{{studentChoicesTable}}}
                  {{else}}
                    {{> studentChoicesTableBlank }}
                  {{/if}}
              </div>
            </div>
        </div>

